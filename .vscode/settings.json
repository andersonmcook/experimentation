{
  "workbench.colorTheme": "Kay Dark",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "constant.other",
        "settings": {
          "foreground": "#ffcc00"
        }
      },
      {
        "scope": "punctuation.section",
        "settings": {
          "foreground": "#ff6600"
        }
      },
      {
        "scope": "parameter.variable.function",
        "settings": {
          "foreground": "#ff00ff"
        }
      },
      {
        "scope": "source",
        "settings": {
          "foreground": "#cc99ff"
        }
      },
      {
        "scope": "string.quoted.double.heredoc",
        "settings": {
          "foreground": "#00ff00"
        }
      }
    ]
  }
}
